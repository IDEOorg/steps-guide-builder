    "2GZ2aOHZvy2CAKAoAkoyE6": {
      "url": "//images.contentful.com/2h2wj50mwc16/2GZ2aOHZvy2CAKAoAkoyE6/95e7a09968f73ad72f0e20df66249855/utility-assistance.svg",
      "fileName": "utility-assistance.svg"
    },
    "4k0iNBaRhmmm6s8UWk2Sw8": {
      "url": "//images.contentful.com/2h2wj50mwc16/4k0iNBaRhmmm6s8UWk2Sw8/47fa9ed1c8c7c3dea1fc7939e624cd89/utility-2.svg",
      "fileName": "utility-2.svg"
    },
    "4ipMdWmSycuUwOAaUg2kQ6": {
      "url": "//images.contentful.com/2h2wj50mwc16/4ipMdWmSycuUwOAaUg2kQ6/bb51cf637a322e8a8426cf607f202e77/utility-subsidy.svg",
      "fileName": "utility-subsidy.svg"
